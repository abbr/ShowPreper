{
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "amd": true,
    "es6": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    "comma-dangle": 1,
    "no-undef": 1,
    "global-strict": 0,
    "no-underscore-dangle": 0,
    "no-console": 1,
    "no-unused-vars": [1, { "ignoreRestSiblings": true }],
    "no-trailing-spaces": [1, { "skipBlankLines": true }],
    "no-unreachable": 1,
    "no-alert": 0,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1
  }
}